{
    "ctor":"Modal",
    "props":{
       "id":"formsModal",
       "size":"ModalSize.LARGE",
       "title":"Select Modal",
       "components":[
        {
            "ctor":"Repeater",
            "props":{
            "id":"modalList",
            "dataProvider": "{?componentList}",
            "rendering":{
                "direction":"horizontal"
            },
            "components":[
                {
                    "ctor":"Container",
                    "props":{
                        "id":"form",
                        "draggable":true,
                        "classes":[
                        "border",
                        "comp_side"
                        ],
                        "css":{
                        "borderRadius":"5px",
                        "margin":"2px 2px"
                        },
                        "width":130,
                        "height":80,
                        "type":"",
                        "components":[
                        {
                            "ctor":"Label",
                            "props":{
                                "id":"formLabel",
                                "label":"{label}",
                                "css":{
                                    "fontSize":"15px",
                                    "marginTop":"20px",
                                    "fontWeight":"bold"
                                }
                            }
                        }
                        ]
                    }
                }
            ]
            }
        }
       ]
    }
 }