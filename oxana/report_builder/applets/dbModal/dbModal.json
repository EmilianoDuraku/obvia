{
  "ctor": "Modal",
  "props": {
    "id": "dbModal",
    "title": "Choose Connection",
    "components": [
      {
        "ctor": "Container",
        "props": {
          "id": "dbModalContainer",
          "type": "",
          "components": [
            {
              "ctor": "Container",
              "props": {
                "id": "dbModalDatasourceContainer",
                "type": "",
                "classes": ["mb-3", "row", "dbModalDatasourceContainer"],
                "components": [
                  {
                    "ctor": "Label",
                    "props": {
                      "label": "Choose A Connection From The System",
                      "labelType": "label",
                      "id": "dbModalDatasourceTitle",
                      "classes": ["col-6"],
                      "css": {
                        "marginBottom": "0px"
                      }
                    }
                  },
                  {
                    "ctor": "DropDown",
                    "props": {
                      "id": "dbModalDatasourceDropdown",
                      "labelField": "datasource_name",
                      "valueField": "datasource_name",
                      "label": "Datasource",
                      "classes": [
                        "fas",
                        "fas-sort-down",
                        "btn-default",
                        "col-6"
                      ],
                      "css": {
                        "verticalAlign": "top"
                      }
                    }
                  }
                ]
              }
            },
            {
              "ctor": "Container",
              "props": {
                "id": "dbModalDataviewContainer",
                "type": "",
                "classes": ["mb-3", "row", "dbModalDataviewContainer"],
                "components": [
                  {
                    "ctor": "Label",
                    "props": {
                      "label": "Choose A Table From The Database",
                      "labelType": "label",
                      "id": "dbModalDataviewTitle",
                      "classes": ["col-6"],
                      "css": {
                        "marginBottom": "0px"
                      }
                    }
                  },
                  {
                    "ctor": "DropDown",
                    "props": {
                      "id": "dbModalDataviewDropdown",
                      "labelField": "name",
                      "valueField": "name",
                      "label": "Dataview",
                      "classes": [
                        "fas",
                        "fas-sort-down",
                        "btn-default",
                        "col-6"
                      ],
                      "css": {
                        "verticalAlign": "top"
                      }
                    }
                  }
                ]
              }
            },
            {
              "ctor": "Container",
              "props": {
                "id": "dbModalRepeaterContainer",
                "classes": ["dbModalRepeaterContainer", "mb-3"],
                "components": [
                  {
                    "ctor": "Repeater",
                    "props": {
                      "id": "dbModalRepeaterLeft",
                      "classes": ["col-4", "dbModalRepeater"],
                      "rendering": {
                        "direction": "vertical"
                      },
                      "dataProvider": [],
                      "components": [
                        {
                          "ctor": "Label",
                          "props": {
                            "id": "dbModalRepeaterLeftLabel",
                            "labelType": "label",
                            "label": "{description}"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ctor": "Container",
                    "props": {
                      "id": "componentContainerMiddle",
                      "classes": ["col-4", "componentContainerMiddle"],
                      "rendering": {
                        "direction": "vertical"
                      },
                      "components": [
                        {
                          "ctor": "Button",
                          "props": {
                            "id": "dbModalContainerMiddleButtonRight",
                            "classes": [
                              "mb-1",
                              "btn",
                              "btn-primary",
                              "fas",
                              "fa-angle-right"
                            ]
                          }
                        },
                        {
                          "ctor": "Button",
                          "props": {
                            "id": "dbModalContainerMiddleButtonLeft",
                            "classes": [
                              "mb-1",
                              "btn",
                              "btn-primary",
                              "fas",
                              "fa-angle-left"
                            ]
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ctor": "Repeater",
                    "props": {
                      "id": "dbModalRepeaterRight",
                      "classes": ["col-4", "dbModalRepeater"],
                      "rendering": {
                        "direction": "vertical"
                      },
                      "dataProvider": [],
                      "components": [
                        {
                          "ctor": "Label",
                          "props": {
                            "id": "dbModalRepeaterRightLabel",
                            "labelType": "label",
                            "label": "{description}"
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            },
            {
              "ctor": "Container",
              "props": {
                "id": "dbModalAddFieldsContainer",
                "classes": ["row", "dbModalAddFieldsContainer"],
                "components": [
                  {
                    "ctor": "Label",
                    "props": {
                      "id": "dbModalAddFieldsLabel",
                      "classes": ["dbModalAddFieldsLabel", "col-6"],
                      "label": "Do you want to add data?"
                    }
                  },
                  {
                    "ctor": "Container",
                    "props": {
                      "id": "dbModalAddFieldsButtonContainer",
                      "classes": ["col-6"],
                      "components": [
                        {
                          "ctor": "Button",
                          "props": {
                            "id": "dbModalAddFieldsButton",
                            "classes": [
                              "btn",
                              "btn-secondary",
                              "fas",
                              "fa-plus",
                              "dbModalAddFieldsButton"
                            ],
                            "label": "Add Fields"
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  }
}
