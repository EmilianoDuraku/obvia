{
  "ctor": "Modal",
  "props": {
    "id": "jsEditorModal",
    "title": "Javascript Expression Editor",
    "size": "ModalSize.SMALL",
    "components": {
      "modalBody": [
        {
          "ctor": "Container",
          "props": {
            "id": "editorBodyContainer",
            "classes": ["editorBodyContainer"],
            "components": [
              {
                "ctor": "Container",
                "props": {
                  "id": "editorContainer",
                  "components": [
                    {
                      "ctor": "Label",
                      "props": {
                        "id": "editorLabel",
                        "visible": false
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ],
      "modalFooter": [
        {
          "ctor": "Button",
          "props": {
            "id": "finishButton",
            "classes": ["btn", "btn-primary", "mx-2"],
            "label": "Finish"
          }
        },
        {
          "ctor": "Button",
          "props": {
            "id": "cancelButton",
            "classes": ["btn", "btn-secondary", "mx-2"],
            "label": "Cancel"
          }
        }
      ]
    }
  }
}
